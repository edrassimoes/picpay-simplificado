	"item": [
		{
			"name": "CREATE COMMON USER",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Usuário COMMON criado com sucesso\", function () {",
							"    pm.response.to.have.status(201);",
							"});",
							"",
							"pm.test(\"Response deve conter ID do usuário\", function () {",
							"    const json = pm.response.json();",
							"    pm.expect(json).to.have.property(\"id\");",
							"    pm.expect(json.id).to.be.a(\"number\");",
							"});",
							"",
							"pm.test(\"Response deve conter dados corretos\", function () {",
							"    const json = pm.response.json();",
							"    pm.expect(json.name).to.eql(\"Pedro\");",
							"    pm.expect(json.userType).to.eql(\"COMMON\");",
							"});",
							"",
							"pm.test(\"Password não deve ser retornado\", function () {",
							"    const json = pm.response.json();",
							"    pm.expect(json).to.not.have.property(\"password\");",
							"});"
						],
						"type": "text/javascript",
						"packages": {},
						"requests": {}
					}
				}
			],
