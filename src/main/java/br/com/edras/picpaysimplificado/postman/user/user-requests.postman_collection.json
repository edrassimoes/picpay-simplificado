		{
			"name": "GET USER BY ID",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Usuário retornado com sucesso\", function () {",
							"    pm.response.to.have.status(200);",
							"});",
							"",
							"pm.test(\"Response deve conter dados do usuário\", function () {",
							"    const json = pm.response.json();",
							"    pm.expect(json).to.have.property(\"id\");",
							"    pm.expect(json).to.have.property(\"name\");",
							"    pm.expect(json).to.have.property(\"userType\");",
							"});",
							"",
							"pm.test(\"ID deve corresponder ao solicitado\", function () {",
							"    const json = pm.response.json();",
							"    const requestedId = pm.request.url.getPath().split(\"/\").pop();",
							"    pm.expect(json.id.toString()).to.eql(requestedId);",
							"});",
							"",
							"pm.test(\"Password não deve ser retornado\", function () {",
							"    const json = pm.response.json();",
							"    pm.expect(json).to.not.have.property(\"password\");",
							"});"
						],
						"type": "text/javascript",
						"packages": {},
						"requests": {}
					}
				}
			],
			"request": {
				"method": "GET",
				"header": [],
