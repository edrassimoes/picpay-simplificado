		{
			"name": "GET ALL USERS",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Lista de usuários retornada com sucesso\", function () {",
							"    pm.response.to.have.status(200);",
							"});",
							"",
							"pm.test(\"Response deve ser um array\", function () {",
							"    const json = pm.response.json();",
							"    pm.expect(json).to.be.an(\"array\");",
							"});",
							"",
							"pm.test(\"Usuários devem conter campos obrigatórios\", function () {",
							"    const json = pm.response.json();",
							"    if (json.length > 0) {",
							"        const user = json[0];",
							"        pm.expect(user).to.have.property(\"id\");",
							"        pm.expect(user).to.have.property(\"name\");",
							"        pm.expect(user).to.have.property(\"userType\");",
							"    }",
							"});",
							"",
							"pm.test(\"Password não deve ser retornado em nenhum usuário\", function () {",
							"    const json = pm.response.json();",
							"    json.forEach(user => {",
							"        pm.expect(user).to.not.have.property(\"password\");",
							"    });",
							"});"
						],
